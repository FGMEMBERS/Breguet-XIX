    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>
  
  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Aircraft/Breguet-XIX/Models/Effects/Glass/glass</inherits-from>
